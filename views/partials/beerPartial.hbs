<div class="beer">
  <div id="img-party">
    <div class="img-beer">
      <img src="{{this.image_url}}" alt="beer image cap">
    </div>
    <div class="img-vieja">
      <img src="/images/vieja-beer.gif">
    </div>

  </div>
  <div class="beer-body">
    <h4 class="beer-title">{{this.name}}</h4>
    <h5 class="beer-tagline">{{this.tagline}}</h5>
    <p class="beer-text">{{this.description}}</p>
  </div>
</div>